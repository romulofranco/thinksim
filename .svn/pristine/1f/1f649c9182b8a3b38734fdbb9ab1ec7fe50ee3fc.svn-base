.ui-widget {
	
	border: none;
}
.ui-widget-header .ui-state-default {	
	border-top: ridge 1px;
	border-left: none;
	border-right: thick ;
	border-bottom: none;
}

#ui-widget-content{
	boder: none;
	border-radius: 0px;
}
.ui-datatable .ui-datatable-data td {
    border-bottom: white;
    border-right: white;    
}

