}

/* Correct left margin */
.navbar-header {
	padding-left: 15px;
}

#ui-growl1 {
	position: fixed;
	top: 180px;
}

.navbar .navbar-toggle {
	position: relative;
	z-index: 2;
	min-height: 34px;
	padding: 9px 5px;
	border-color: transparent
}

.navbar-inner {
    background:transparent;
}

