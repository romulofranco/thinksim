.navbar .navbar-toggle:not (.collapsed ) .bottom-bar .fixar {
	position: absolute;
	bottom: 46%;
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	transform: rotate(-45deg)
}

@media screen and (max-width:767px) {
	#navbar {
		display: none;
	}
	.fixar {
		display: block;
	}
}